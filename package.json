  "repository": {
    "type": "git",
    "url": "git+https://github.com/solvie/literate-parakeet.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/solvie/literate-parakeet/issues"
  },
  "homepage": "https://github.com/solvie/literate-parakeet#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.11",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
